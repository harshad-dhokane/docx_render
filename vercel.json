{
  "version": 2,  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**", "views/**"]
      }
    }
  ],  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_KEY": "@supabase_service_key",
    "SUPABASE_KEY": "@supabase_key",
    "SUPABASE_TEMPLATES_BUCKET": "@supabase_templates_bucket",
    "SUPABASE_GENERATED_BUCKET": "@supabase_generated_bucket"
  }
}